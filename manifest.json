{
  "manifest_version": 3,
  "name": "FlexBookmark",
  "version": "1.1.0",
  "description": "Advanced bookmark management with modern UI",
  "key": "",
  "icons": {
    "128": "assets/icons/icon.png"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "tabs",
    "identity"
  ],
  "oauth2": {
    "client_id": "661697300169-fn99f3i4gnegds1qdruj4n3t0ib1sunp.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "email",
      "profile",
      "https://www.googleapis.com/auth/tasks",
      "https://www.googleapis.com/auth/tasks.readonly",
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "host_permissions": [
    "https://www.googleapis.com/*",
    "https://api.open-meteo.com/*",
    "https://tasks.googleapis.com/*",
    "https://oauth2.googleapis.com/*"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; connect-src 'self' https://tasks.googleapis.com https://www.googleapis.com https://api.open-meteo.com https://oauth2.googleapis.com https://*.gstatic.com;"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "src/content/newtab/*.js",
        "src/content/newtab/components/*.js",
        "src/content/utils/*.js",
        "assets/styles/*.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}